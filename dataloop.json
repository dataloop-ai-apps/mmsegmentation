    "computeConfigs": [
      {
        "name": "mmsegmentation-deploy",
        "runtime": {
          "podType": "highmem-xs",
          "concurrency": 1,
          "runnerImage": "shadimahameeddl/mm-dl:latest",
          "autoscaler": {
            "type": "rabbitmq",
            "minReplicas": 0,
            "maxReplicas": 2
          }
        }
      }
    ],
    "modules": [
      {
        "name": "mmsegmentation-module",
        "entryPoint": "model_adapter.py",
        "className": "MMSegmentation",
        "computeConfig": "mmsegmentation-deploy",
        "description": "MMSegmentation Module",
        "initInputs": [
          {
            "type": "Model",
            "name": "model_entity"
          }
        ],
        "functions": [
          {
            "name": "predict_items",
            "input": [
              {
                "type": "Item[]",
                "name": "items",
                "description": "List of items to run inference on"
              }
            ],
            "output": [
              {
                "type": "Annotation[]",
                "name": "annotations",
                "description": "List of predictions"
              }
            ],
            "displayName": "Predict Items",
            "displayIcon": "",
            "description": "MMSegmentation predict function"
          },
          {
            "name": "predict_dataset",
            "input": [
              {
                "type": "Dataset",
                "name": "dataset",
                "description": "The input dataset of the items required for prediction."
              },
              {
                "type": "Json",
                "name": "filters",
                "description": "The DQL in json format to get all the items required for prediction."
              }
            ],
            "output": [],
            "displayName": "Predict Dataset",
            "displayIcon": "",
            "description": "Inference function of the model on a dataset."
          }
        ]
      }
    ],
    "models": [
      {
        "name": "mmlab-mmsegmentation",
        "moduleName": "mmsegmentation-module",
        "scope": "project",
        "status": "trained",
