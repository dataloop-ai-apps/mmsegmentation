          "config_file": "pspnet_r50-d8_4xb4-80k_coco-stuff164k-512x512.py",
          "checkpoint_file": "pspnet_r50-d8_512x512_4x4_80k_coco-stuff164k_20210707_152034-0e41b2db.pth",
          "device": "cpu",
          "confidence_thr": 0.4
        },
        "supportedMethods": {
          "load": true,
          "predict": true,
          "train": false,
